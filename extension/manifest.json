{
  "manifest_version": 3,
  "name": "Toastmasters VPE Exporter",
  "version": "1.0",
  "description": "Export educational report data from Toastmasters website.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.toastmasters.org/*"],
      "js": ["content.js"]
    }
  ]
}